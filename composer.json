{
    "name": "joynala/web-installer",
    "description": "This is a laravel web installer and builder",
    "license": "MIT",
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Abedin\\WebInstaller\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Joynal Abedin",
            "email": "abedin.dev@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/console": "^9.0|^10.0",
        "illuminate/support": "^9.0|^10.0",
        "vendor/web-installer": "dev-main"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Abedin\\WebInstaller\\Providers\\AppServiceProvider"
            ]
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@php artisan vendor:publish --tag=web-installer-config --force"
        ]
    }
}
